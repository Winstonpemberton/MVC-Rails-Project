<h1>Sign In</h1>
<%= form_tag("/signin", method: "post") do %>
  <p>username: <%= text_field_tag :name%></p>

  <p>Password: <%= password_field_tag :password %></p>
  <%= submit_tag "Sign In" %>

  <%= link_to(controller: 'users', action: 'new') { 'sign up' } %>
  <p><%= link_to 'Signin with GitHub', github_auth_path%>.</p>
<% end %>


